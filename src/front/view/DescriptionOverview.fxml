<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox spacing="25.0" xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1" fx:controller="front.view.DescriptionOverviewController">
   <children>
      <VBox spacing="25.0">
         <children>
            <Label text="Description" />
            <Label fx:id="description" prefWidth="184.0" text="Label" />
         </children>
      </VBox>
      <VBox spacing="25.0">
         <children>
            <Label fx:id="physiqueLabel" text="Description physique" />
            <Label fx:id="physique" text="Label" />
         </children>
      </VBox>
      <Label text="Listes des competences" />
      <HBox spacing="20.0">
         <children>
            <TableView fx:id="tableCompetence" minWidth="100.0">
               <columns>
                  <TableColumn fx:id="nameCol" minWidth="100.0" prefWidth="-1.0" text="Name" />
               </columns>
               <columnResizePolicy>
                  <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
               </columnResizePolicy>
            </TableView>
            <Label fx:id="descriptionComp" prefHeight="363.0" text="Description de la compétence sélectionner" textAlignment="JUSTIFY" />
         </children>
      </HBox>
   </children>
</VBox>
